{
  "name": "line-node",
  "sideEffects": false,
  "version": "0.0.3",
  "description": "LINE SDK for TypeScript",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "typings": "types/index.d.ts",
  "scripts": {
    "test": "ava",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "rollup": "rollup -c build.js",
    "build": "npm run lint && npm run test && npm run rollup",
    "release": "npm run build && np"
  },
  "engines": {
    "node": ">=10.19.0"
  },
  "dependencies": {
    "got": "^11.8.0",
    "jsonwebtoken": "^8.5.1"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^8.5.0",
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/parser": "^4.6.1",
    "ava": "^3.13.0",
    "eslint": "^7.12.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-tree-shaking": "^1.8.0",
    "np": "^7.4.0",
    "prettier": "^2.2.1",
    "rollup": "^2.33.1",
    "rollup-plugin-typescript2": "^0.29.0",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.0.5"
  },
  "keywords": [
    "line",
    "node",
    "nodejs",
    "line-sdk"
  ],
  "author": "Luca Ban - Mesqueeb",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mesqueeb/line-node/issues"
  },
  "homepage": "https://github.com/mesqueeb/line-node#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mesqueeb/line-node.git"
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "tsconfig-paths/register",
      "ts-node/register"
    ]
  },
  "np": {
    "yarn": false,
    "branch": "production"
  }
}
